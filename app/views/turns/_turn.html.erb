<div id="<%= dom_id turn %>">
  <p>
    <strong>Sucursal:</strong>
    <%= turn.sucursal_id %>
  </p>

  <p>
    <strong>Fecha:</strong>
    <%= turn.fecha %>
  </p>

  <p>
  <strong>Hora:</strong>
  <%= turn.hora.strftime("%H:%M:%S") %>
</p>

  <p>
    <strong>Motivo:</strong>
    <%= turn.reason %>
  </p>

  <p>
    <strong>Estado:</strong>
    <%= translateState(turn.state) %>
  </p>


</div>
