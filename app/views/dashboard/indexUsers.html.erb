<h1> Listado de usuarios: </h1>
<% @users.each do |user| %>
    <div class="col-xl-3 col-md-6 mb-4">
    <div class="card border-left-primary shadow h-100 py-2">
      <div class="card-body">
        <div class="align-items-center">
          
          <%= user.email %>
          <span>
          </span>
        </div>
      </div>
    </div>
  </div>
  <% end %>